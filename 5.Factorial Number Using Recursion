#include <stdio.h>
unsigned int factorial(unsigned int n) {
  
  if (n == 0 || n == 1) {
    return 1;
  } else {
    
    return n * factorial(n - 1);
  }
}

int main() {
  unsigned int num;

  printf("Enter a non-negative number: ");
  scanf("%u", &num);
  unsigned int result = factorial(num);

  printf("Factorial of %u is %u\n", num, result);

  return 0;
}
